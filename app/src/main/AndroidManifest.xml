<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gestionarriendo.gva.natillera">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Usuarios"
            android:label="@string/title_activity_usuarios"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Prestamos"
            android:label="@string/title_activity_prestamos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ControlPagos"
            android:label="@string/title_activity_control_pagos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Listado"
            android:label="@string/title_activity_listado"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>